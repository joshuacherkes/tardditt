<h1>Make a New Account</h1>

<form action="<%= users_url %>" method="post">
	<label>Username
		<input type="text" name="user[username]">
	</label>

	<label>Password
		<input type="password" name="user[password]">
	</label>

	  <input type="hidden"
         name="authenticity_token"
         value="<%= form_authenticity_token %>">


	<input type="submit">
</form>
